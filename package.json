{
  "name": "trygit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "jscsConfig": {
    "preset": "node-style-guide",
    "requireCurlyBraces": null,
    "excludeFiles": [
      "node_modules",
      "/public/javascript/bootstrap*"
    ],
    "fileExtensions": [".js"],
    "maxErrors": null,
    "fix": true
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "pug": "2.0.0-alpha6",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  }
}
